    <owl:Class rdf:about="https://w3id.org/def/microrrelatos#Cortometraje">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/microrrelatos#ObraArtistica"/>
        <rdfs:label xml:lang="es">cortometraje</rdfs:label>
    </owl:Class>
    


