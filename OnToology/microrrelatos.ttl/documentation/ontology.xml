        <rdfs:label xml:lang="es">audio</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/microrrelatos#Audiomicrorrelato -->


    <owl:Class rdf:about="https://w3id.org/def/microrrelatos#Audiomicrorrelato">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/microrrelatos#Audio"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/microrrelatos#Microrrelato"/>
        <rdfs:label xml:lang="es">audiomicrorrelato</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/microrrelatos#Autor -->


    <owl:Class rdf:about="https://w3id.org/def/microrrelatos#Autor">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/microrrelatos#Persona"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/microrrelatos#creaObraArtistica"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/def/microrrelatos#ObraArtistica"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="es">Persona que crea la obra artística</rdfs:comment>
        <rdfs:label xml:lang="es">autor</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/def/microrrelatos#Blog -->


    <owl:Class rdf:about="https://w3id.org/def/microrrelatos#Blog">
        <rdfs:subClassOf rdf:resource="https://w3id.org/def/microrrelatos#PublicacionDigital"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/microrrelatos#blogTieneNombre"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="es">Un medio de publicación en la Web que contiene distintos textos.</rdfs:comment>
        <rdfs:label xml:lang="es">blog</rdfs:label>
    </owl:Class>
    


