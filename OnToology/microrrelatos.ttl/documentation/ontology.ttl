@prefix : <https://w3id.org/def/microrrelatos#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/def/microrrelatos> .

<https://w3id.org/def/microrrelatos> rdf:type owl:Ontology ;
                                      owl:imports <http://purl.org/spar/fabio/2019-02-19> ,
                                                  <http://www.w3.org/2006/time#2016> .

#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/publisher
<http://purl.org/dc/terms/publisher> owl:equivalentProperty :tieneEditorial .


###  https://w3id.org/def/microrrelatos#creaObraArtistica
:creaObraArtistica rdf:type owl:ObjectProperty ;
                   owl:inverseOf :esObraArtisticaCreadaPor ;
                   rdfs:comment "Relación entre el creador y la obra artística"@es ;
                   rdfs:label "crea obra artística"@es .


###  https://w3id.org/def/microrrelatos#esObraArtisticaCreadaPor
:esObraArtisticaCreadaPor rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf <http://purl.org/dc/terms/creator> ;
                          rdfs:comment "Relación entre la obra artística y su creador"@es ;
                          rdfs:label "es obra artística creada por"@es .


###  https://w3id.org/def/microrrelatos#microrrelatoTieneAudio
:microrrelatoTieneAudio rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#part> ;
                        rdfs:label "tiene audio (microrrelato)"@es .


###  https://w3id.org/def/microrrelatos#microrrelatoTieneFotografia
:microrrelatoTieneFotografia rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#part> ;
                             rdfs:label "tiene fotografía (microrrelato)"@es .


###  https://w3id.org/def/microrrelatos#microrrelatoTieneVideo
:microrrelatoTieneVideo rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#part> ;
                        rdfs:label "tiene vídeo (microrrelato)"@es .


###  https://w3id.org/def/microrrelatos#perteneceAPais
:perteneceAPais rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#partOf> ;
                rdfs:label "pertenece a país"@es .


###  https://w3id.org/def/microrrelatos#tieneEditorial
:tieneEditorial rdf:type owl:ObjectProperty ;
                rdfs:label "tiene editorial"@es .


###  https://w3id.org/def/microrrelatos#tieneInicioFinPublicacion
:tieneInicioFinPublicacion rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf <http://www.w3.org/2006/time#hasDateTimeDescription> ;
                           rdfs:label "tiene inicio fin (publicación)"@es .


###  https://w3id.org/def/microrrelatos#tieneMicrorrelatoInsertadoIlustracion
:tieneMicrorrelatoInsertadoIlustracion rdf:type owl:ObjectProperty ;
                                       rdfs:subPropertyOf <http://purl.org/vocab/frbr/core#part> ;
                                       rdfs:label "tiene microrrelato insertado (ilustración)"@es .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> owl:equivalentProperty :obraArtisticaTieneTitulo .


###  https://w3id.org/def/microrrelatos#blogTieneNombre
:blogTieneNombre rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :tieneNombre ;
                 rdfs:label "tiene nombre (blog)"@es .


###  https://w3id.org/def/microrrelatos#blogTieneURL
:blogTieneURL rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf <http://purl.org/spar/fabio/hasURL> ;
              rdfs:label "tiene URL (blog)"@es .


###  https://w3id.org/def/microrrelatos#editorialTieneNombre
:editorialTieneNombre rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf :tieneNombre ;
                      rdfs:label "tiene nombre (editorial)"@es .


###  https://w3id.org/def/microrrelatos#obraArtisticaTieneTitulo
:obraArtisticaTieneTitulo rdf:type owl:DatatypeProperty ;
                          rdfs:label "tiene título (obra artística)"@es .


###  https://w3id.org/def/microrrelatos#personaTieneNombre
:personaTieneNombre rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :tieneNombre ;
                    rdfs:label "tiene nombre (persona)"@es .


###  https://w3id.org/def/microrrelatos#tieneApellidos
:tieneApellidos rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf :tieneNombre ;
                rdfs:label "tiene apellidos"@es .


###  https://w3id.org/def/microrrelatos#tieneNombre
:tieneNombre rdf:type owl:DatatypeProperty ;
             rdfs:label "tiene nombre"@es .


#################################################################
#    Classes
#################################################################

###  http://purl.org/spar/fabio/ArtisticWork
<http://purl.org/spar/fabio/ArtisticWork> owl:equivalentClass :ObraArtistica .


###  http://purl.org/spar/fabio/Blog
<http://purl.org/spar/fabio/Blog> owl:equivalentClass :Blog .


###  http://purl.org/vocab/frbr/core#Person
<http://purl.org/vocab/frbr/core#Person> owl:equivalentClass :Persona .


###  https://w3id.org/def/microrrelatos#Audio
:Audio rdf:type owl:Class ;
       rdfs:subClassOf <http://purl.org/spar/fabio/SoundRecording> ;
       rdfs:label "audio"@es .


###  https://w3id.org/def/microrrelatos#Audiomicrorrelato
:Audiomicrorrelato rdf:type owl:Class ;
                   rdfs:subClassOf :Microrrelato ;
                   rdfs:label "audiomicrorrelato"@es .


###  https://w3id.org/def/microrrelatos#Autor
:Autor rdf:type owl:Class ;
       rdfs:subClassOf :Persona ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :creaObraArtistica ;
                         owl:someValuesFrom :ObraArtistica
                       ] ;
       rdfs:comment "Persona que crea la obra artística"@es ;
       rdfs:label "autor"@es .


###  https://w3id.org/def/microrrelatos#Blog
:Blog rdf:type owl:Class ;
      rdfs:subClassOf :PublicacionDigital ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :blogTieneNombre ;
                        owl:someValuesFrom xsd:string
                      ] ;
      rdfs:comment "Un medio de publicación en la Web que contiene distintos textos."@es ;
      rdfs:label "blog"@es .


###  https://w3id.org/def/microrrelatos#Ciudad
:Ciudad rdf:type owl:Class ;
        rdfs:subClassOf :Localidad ;
        rdfs:label "ciudad"@es .


###  https://w3id.org/def/microrrelatos#Cortometraje
:Cortometraje rdf:type owl:Class ;
              rdfs:subClassOf :ObraArtistica ;
              rdfs:label "cortometraje"@es .


###  https://w3id.org/def/microrrelatos#Editorial
:Editorial rdf:type owl:Class ;
           rdfs:subClassOf <http://purl.org/vocab/frbr/core#CorporateBody> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :editorialTieneNombre ;
                             owl:someValuesFrom xsd:string
                           ] ;
           rdfs:comment "No confundir con \"editorial\" en el sentido de artículo firmado por la propia editorial, que es un término procedente de FABIO."@es ;
           rdfs:label "editorial"@es .


###  https://w3id.org/def/microrrelatos#EspacioRadiofonico
:EspacioRadiofonico rdf:type owl:Class ;
                    rdfs:subClassOf :PublicacionDigital ;
                    rdfs:label "espacio radiofónico"@es .


###  https://w3id.org/def/microrrelatos#EspacioTelevisivo
:EspacioTelevisivo rdf:type owl:Class ;
                   rdfs:subClassOf :PublicacionDigital ;
                   rdfs:label "espacio televisivo"@es .


###  https://w3id.org/def/microrrelatos#Fotografia
:Fotografia rdf:type owl:Class ;
            rdfs:subClassOf <http://purl.org/spar/fabio/StillImage> ;
            rdfs:label "fotografía"@es .


###  https://w3id.org/def/microrrelatos#Hombre
:Hombre rdf:type owl:Class ;
        rdfs:subClassOf :Persona ;
        rdfs:label "hombre"@es .


###  https://w3id.org/def/microrrelatos#IlustracionConMicrorrelatoInsertado
:IlustracionConMicrorrelatoInsertado rdf:type owl:Class ;
                                     rdfs:subClassOf :ObraArtistica ,
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :tieneMicrorrelatoInsertadoIlustracion ;
                                                       owl:someValuesFrom :Microrrelato
                                                     ] ;
                                     rdfs:label "ilustración con microrrelato insertado"@es .


###  https://w3id.org/def/microrrelatos#Localidad
:Localidad rdf:type owl:Class ;
           rdfs:subClassOf :Lugar ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :perteneceAPais ;
                             owl:someValuesFrom :Pais
                           ] ;
           rdfs:label "localidad"@es .


###  https://w3id.org/def/microrrelatos#Lugar
:Lugar rdf:type owl:Class ;
       rdfs:label "lugar"@es .


###  https://w3id.org/def/microrrelatos#Microrrelato
:Microrrelato rdf:type owl:Class ;
              rdfs:subClassOf :ObraArtistica ;
              rdfs:comment "El microrrelato es un relato de muy pocas líneas que, por la brevedad y precisión en su vocabulario, crea la intensidad suficiente para turbar al lector (http://www.ies-ramonycajal.com/index.php/departamentos-mainmenu-32/dpto-lengua-espa-mainmenu-107/571-que-es-y-como-escribir-un-microrrelato)."@es ;
              rdfs:label "microrrelato"@es .


###  https://w3id.org/def/microrrelatos#MicrorrelatoConAudio
:MicrorrelatoConAudio rdf:type owl:Class ;
                      rdfs:subClassOf :Microrrelato ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :microrrelatoTieneAudio ;
                                        owl:someValuesFrom :Audio
                                      ] ;
                      rdfs:label "microrrelato con audio"@es .


###  https://w3id.org/def/microrrelatos#MicrorrelatoConMinificcion
:MicrorrelatoConMinificcion rdf:type owl:Class ;
                            rdfs:subClassOf :Microrrelato ;
                            rdfs:label "microrrelato con minificción"@es .


###  https://w3id.org/def/microrrelatos#MicrorrelatoConVideo
:MicrorrelatoConVideo rdf:type owl:Class ;
                      rdfs:subClassOf :Microrrelato ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :microrrelatoTieneVideo ;
                                        owl:someValuesFrom :Video
                                      ] ;
                      rdfs:label "microrrelato con vídeo"@es .


###  https://w3id.org/def/microrrelatos#Mujer
:Mujer rdf:type owl:Class ;
       rdfs:subClassOf :Persona ;
       rdfs:label "mujer"@es .


###  https://w3id.org/def/microrrelatos#ObraArtistica
:ObraArtistica rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty :esObraArtisticaCreadaPor ;
                                 owl:someValuesFrom :Autor
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :obraArtisticaTieneTitulo ;
                                 owl:someValuesFrom xsd:string
                               ] ;
               rdfs:comment "Trabajo como arte en su sentido más amplio, incluyendo trabajos literarios, obras musicales, arte visual, etc. (traducido de http://purl.org/spar/fabio/ArtisticWork)"@es ;
               rdfs:label "obra artística"@es .


###  https://w3id.org/def/microrrelatos#Pais
:Pais rdf:type owl:Class ;
      rdfs:subClassOf :Lugar ;
      rdfs:label "país"@es .


###  https://w3id.org/def/microrrelatos#Persona
:Persona rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :personaTieneNombre ;
                           owl:someValuesFrom xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :tieneApellidos ;
                           owl:someValuesFrom xsd:string
                         ] ;
         rdfs:label "persona"@es .


###  https://w3id.org/def/microrrelatos#Publicacion
:Publicacion rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Restriction ;
                               owl:onProperty :tieneInicioFinPublicacion ;
                               owl:someValuesFrom <http://www.w3.org/2006/time#DateTimeInterval>
                             ] ;
             rdfs:label "publicación"@es .


###  https://w3id.org/def/microrrelatos#PublicacionDigital
:PublicacionDigital rdf:type owl:Class ;
                    rdfs:subClassOf :Publicacion ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :blogTieneURL ;
                                      owl:someValuesFrom xsd:anyURI
                                    ] ;
                    rdfs:label "publicación digital"@es .


###  https://w3id.org/def/microrrelatos#Pueblo
:Pueblo rdf:type owl:Class ;
        rdfs:subClassOf :Localidad ;
        rdfs:label "pueblo"@es .


###  https://w3id.org/def/microrrelatos#RemediacionAudiovisual
:RemediacionAudiovisual rdf:type owl:Class ;
                        rdfs:subClassOf :PublicacionDigital ;
                        rdfs:label "remediación audiovisual"@es .


###  https://w3id.org/def/microrrelatos#RevistaDigital
:RevistaDigital rdf:type owl:Class ;
                rdfs:subClassOf :PublicacionDigital ;
                rdfs:label "revista digital"@es .


###  https://w3id.org/def/microrrelatos#TallerDigital
:TallerDigital rdf:type owl:Class ;
               rdfs:subClassOf :PublicacionDigital ;
               rdfs:label "taller digital"@es .


###  https://w3id.org/def/microrrelatos#Video
:Video rdf:type owl:Class ;
       rdfs:subClassOf <http://purl.org/spar/fabio/MovingImage> ;
       rdfs:label "video"@es .


###  https://w3id.org/def/microrrelatos#microrrelatoConFotografia
:microrrelatoConFotografia rdf:type owl:Class ;
                           rdfs:subClassOf :Microrrelato ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty :microrrelatoTieneFotografia ;
                                             owl:someValuesFrom :Fotografia
                                           ] ;
                           rdfs:label "microrrelato con fotografía"@es .


###  https://w3id.org/def/microrrelatos#microrrelatoEnLaWeb
:microrrelatoEnLaWeb rdf:type owl:Class ;
                     rdfs:subClassOf :Microrrelato ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <http://purl.org/spar/fabio/hasURL> ;
                                       owl:someValuesFrom xsd:anyURI
                                     ] ;
                     rdfs:label "microrrelato en la Web"@es .


###  https://w3id.org/def/microrrelatos#publicacionImpresa
:publicacionImpresa rdf:type owl:Class ;
                    rdfs:subClassOf :Publicacion ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :tieneEditorial ;
                                      owl:allValuesFrom :Editorial
                                    ] ;
                    rdfs:label "publicación impresa"@es .


