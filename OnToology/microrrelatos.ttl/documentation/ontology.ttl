:creaObraArtistica rdf:type owl:ObjectProperty ;
                   owl:inverseOf :esObraArtisticaCreadaPor ;
                   rdfs:comment "Relación entre el creador y la obra artística"@es ;
                   rdfs:label "crea obra artística"@es .


