        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="es">Condición orgánica, masculina o femenina, de los animales y las plantas (https://dle.rae.es/sexo?m=form).</rdfs:comment>
    </owl:DatatypeProperty>
    


