        <rdfs:domain rdf:resource="https://w3id.org/def/microrrelatos#Persona"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="es">Nombre que se da a una persona cuando se bautiza o el que se le adjudica por elección para identificarla junto a los apellidos (https://dle.rae.es/nombre?m=form#7JmtrIe).</rdfs:comment>
        <rdfs:label xml:lang="es">nombre de pila</rdfs:label>
    </owl:DatatypeProperty>
    


