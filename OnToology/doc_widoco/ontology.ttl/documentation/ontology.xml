    <owl:DatatypeProperty rdf:about="https://w3id.org/def/microrrelatos#tipologia">
        <rdfs:domain rdf:resource="https://w3id.org/def/microrrelatos#Microrrelato"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="es">Cómo se muestra el microrrelato: fotografía + texto, microrrelato visual animado, etc.</rdfs:comment>
        <rdfs:label xml:lang="es">tipología</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/def/microrrelatos#titulo -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/def/microrrelatos#titulo">
        <rdfs:domain rdf:resource="https://w3id.org/def/microrrelatos#ObraArtistica"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="es">Palabra o frase con que se da a conocer el nombre o asunto de una obra o de cada una de las partes o divisiones de un escrito (https://dle.rae.es/t%C3%ADtulo?m=form).</rdfs:comment>
        <rdfs:label xml:lang="es">título</rdfs:label>
    </owl:DatatypeProperty>
    


