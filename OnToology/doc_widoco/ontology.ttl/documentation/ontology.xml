        <rdfs:domain rdf:resource="http://purl.org/vocab/frbr/core#Endeavour"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment xml:lang="es">Un identificador, en forma de un localizador universal de recursos HTTP (URL), para un recurso particular en la World Wide Web.</rdfs:comment>
        <rdfs:label xml:lang="es">tiene URL</rdfs:label>
    </owl:DatatypeProperty>
    


